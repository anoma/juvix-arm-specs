--- An instantiation of a proving System for compliance proofs.
module Anoma.Compliance.ProvingSystem;

import Stdlib.Prelude open;
import Anoma.Compliance.Types open;
import Anoma.Utils open;

prove
  (provingKey : ProvingKey)
  (publicInputs : Instance)
  (privateInputs : Witness)
  : Proof := MISSING_ANOMA_BUILTIN;

verify (proofRecord : ProofRecord) : Bool := MISSING_ANOMA_BUILTIN;
